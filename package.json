{
  "name": "robosite",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for RoboSite web, API, and worker.",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "docker compose up --build",
    "api": "npm --workspace @robosite/api run dev",
    "worker": "npm --workspace @robosite/worker run dev",
    "web": "npm --workspace @robosite/web run dev"
  }
}
